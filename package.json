{
  "name": "destiny-child-tools",
  "version": "1.0.0",
  "description": "Centralized repository for various Destiny Child game related tools",
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "4.5.1",
    "copy-to-clipboard": "3.2.0",
    "cypress": "3.6.0",
    "history": "4.9.0",
    "immutable": "4.0.0-rc.12",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native-cli": "^2.0.1",
    "react-native-material-ui": "^1.30.1",
    "react-redux": "7.1.0",
    "react-select": "3.0.4",
    "redux": "4.0.1",
    "redux-first-router": "2.1.2",
    "redux-first-router-link": "2.1.1",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/node": "7.6.3",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.6.3",
    "@babel/register": "7.6.2",
    "babel-loader": "8.0.6",
    "body-parser": "1.19.0",
    "concurrently": "4.1.0",
    "css-loader": "3.0.0",
    "eslint": "5.13.0",
    "eslint-plugin-react": "7.13.0",
    "expo-cli": "^3.4.1",
    "express": "4.17.1",
    "express-fileupload": "1.1.5",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "jimp": "0.8.5",
    "react-hot-loader": "4.12.15",
    "style-loader": "0.23.1",
    "sync-request": "6.1.0",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.2"
  },
  "scripts": {
    "start": "NODE_ENV=development yarn server",
    "client": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js --mode development",
    "server": "npx babel-node ./server/index.js",
    "server-prod": "npx babel-node ./server/prod.js",
    "import-files": "npx babel-node ./server/import.js",
    "dist": "NODE_ENV=production babel-node ./server/update-json.js && webpack --config ./webpack.config.js --mode production && node ./docs/live2d/generate-json.js && node ./server/generate-html.js && yarn previews",
    "test": "npx cypress run",
    "release": "yarn dist && git add . && git commit -sm \"Release\" && git push",
    "auto-crop": "npx babel-node ./server/auto-crop.js",
    "previews": "yarn auto-crop && npx cypress run --spec=cypress/integration/previews.spec.js &&  yarn auto-crop || yarn auto-crop"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-lokicoder:lokicoder/destiny-child-tools.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "LokiCoder, Arsylk",
  "license": "MIT"
}
